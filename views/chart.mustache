<div class="panel col-md-8 border rounded p-2">
    <canvas id="chart"></canvas>
</div>
<script>
    var ctx = document.getElementById('chart').getContext('2d');
    var chart = new Chart(ctx, {
        type: 'line',
        data: {
            labels: {{{ labels }}},
    datasets: [{
        label: {{{ label }}},
        data: {{ data }},
        fill: false,
        borderColor: 'rgb(75, 192, 192)',
        tension: 0.1
          }]
        },
    options: {
        scales: {
            y: {
                suggestedMin: {{ suggestedMin }}
            }
        },
        responsive: true,
        maintainAspectRatio: true,
        scales: {
            xAxes: [{
                maxTicksLimit: 5
            }]
        }
    }
      });
</script>